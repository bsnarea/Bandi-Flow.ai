{
  "name": "OpenWeather",
  "url": "https://api.openweathermap.org/data/2.5",
  "key": "YOUR_API_KEY_HERE",
  "prefix_id": "weather",
  "tags": ["Weather", "OpenWeather"],
  "models": [
    {
      "id": "current-weather-city",
      "name": "Current Weather (City)",
      "endpoint": "/weather?q={city}&units=metric&lang=en&appid={key}",
      "description": "Get current weather by city name"
    },
    {
      "id": "current-weather-coords",
      "name": "Current Weather (Coordinates)",
      "endpoint": "/weather?lat={lat}&lon={lon}&units=metric&lang=en&appid={key}",
      "description": "Get current weather by latitude/longitude"
    },
    {
      "id": "forecast",
      "name": "5-Day Forecast",
      "endpoint": "/forecast?q={city}&units=metric&lang=en&appid={key}",
      "description": "Get 5-day weather forecast by city name"
    }
  ]
}
