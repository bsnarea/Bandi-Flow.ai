/지역 제한 없는 조회 코드
{
  "name": "OpenWeather",
  "url": "https://api.openweathermap.org/data/2.5",
  "key": "YOUR_API_KEY_HERE",
  "prefix_id": "weather",
  "tags": ["Weather", "OpenWeather"],
  "models": [
    {
      "id": "current-weather-city-country",
      "name": "Current Weather (City, Country)",
      "endpoint": "/weather?q={city},{country}&units=metric&lang=en&appid={key}",
      "description": "Get current weather by city name and country code (ISO 3166)"
    },
    {
      "id": "current-weather-coords",
      "name": "Current Weather (Coordinates)",
      "endpoint": "/weather?lat={lat}&lon={lon}&units=metric&lang=en&appid={key}",
      "description": "Get current weather by latitude/longitude"
    },
    {
      "id": "forecast-city-country",
      "name": "5-Day Forecast (City, Country)",
      "endpoint": "/forecast?q={city},{country}&units=metric&lang=en&appid={key}",
      "description": "Get 5-day weather forecast by city name and country code (ISO 3166)"
    }
  ]
}


